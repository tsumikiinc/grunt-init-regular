{
  // 定義する前に変数を使用することを禁止(ホスティング)
  "latedef": "nofunc",

  // コンストラクタ関数は必ず先頭を大文字に 通常の関数は先頭を小文字に
  "newcap": true,

  // 文字列をシングルクォートかダブルクォートどちらかに統一
  "quotmark": true,

  // var 宣言されていない変数の使用禁止
  "undef": true,

  // 厳密等価 === ではなく 等価 == を使っても警告を出さない
  "eqnull": true,

  // プロパティの参照でドット記法 obj.hoge が使える時でもブラケット記法 obj['hoge'] での参照を許可
  "sub": true,

  // 以下、使用するライブラリ等で適宜変更
  "browser": true,
  "jquery": true,
  "node": true,
  "devel": true
}